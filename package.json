{
  "name": "bookmyride",
  "version": "1.0.0",
  "description": "backend for a taxi booking applocation",
  "main": "server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start-prod": "npm run build && node ./build/server.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "npx babel -d ./build ./src -s",
    "start-dev": "node --exec babel-node src/server.js",
    "server": "nodemon --exec babel-node src/server.js",
    "postinstall": "npm install",
    "start": "npm run start-prod"
  },
  "author": "Vansham Aggarwal",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-preset-minify": "^0.5.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "config": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-jwt": "^6.0.0",
    "express-list-endpoints": "^4.0.1",
    "express-validator": "^6.9.2",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.9",
    "nodemon": "^2.0.2"
  }
}
